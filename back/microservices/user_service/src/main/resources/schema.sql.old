DROP TABLE IF EXISTS `USERMODEL`;
CREATE TABLE `USERMODEL` (
    `id` INTEGER AUTO_INCREMENT,
    `login` VARCHAR DEFAULT NULL,
    `pwd` VARCHAR DEFAULT NULL,
    `account` FLOAT,
    `lastName` VARCHAR DEFAULT NULL,
    `surName` VARCHAR DEFAULT NULL,
    `email` VARCHAR DEFAULT NULL,

  PRIMARY KEY (`id`)
);

CREATE TABLE `CARDMODEL` (
	`id`  INTEGER AUTO_INCREMENT,
	`name` VARCHAR DEFAULT NULL,
	`description` VARCHAR DEFAULT NULL,
	`family` VARCHAR DEFAULT NULL,
	`affinity` VARCHAR DEFAULT NULL,
	`imgUrl` VARCHAR DEFAULT NULL,
	`smallImgUrl` VARCHAR DEFAULT NULL,
	`energy` FLOAT,
	`hp` FLOAT,
	`defence` FLOAT,
	`attack` FLOAT,
	`price` FLOAT,
	PRIMARY KEY (`id`)
);
